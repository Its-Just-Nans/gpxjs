{
	"name": "@we-gold/gpxjs",
	"author": "Weaver Goldman <we.goldm@gmail.com>",
	"description": "GPX.js is a modern library for parsing GPX files and converting them to GeoJSON.",
	"version": "1.0.0",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/gpxjs.umd.cjs",
	"module": "./dist/gpxjs.js",
	"exports": {
		".": {
			"import": "./dist/gpxjs.js",
			"require": "./dist/gpxjs.umd.cjs"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview"
	},
	"devDependencies": {
		"typescript": "^5.0.2",
		"vite": "^4.3.9"
	},
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"semi": false,
		"useTabs": true
	}
}
